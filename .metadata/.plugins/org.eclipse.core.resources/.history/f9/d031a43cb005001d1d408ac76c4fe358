		String CURDtype = "S";
		Object [] param = {29};
		
		/*
		 * String sqlQuery = "insert into userInfo(age, name) values(?, ?)"; String
		 * CURDtype = "I"; Object [] param = {29, "jh"};
		 */	
		jdbcCon.dbConnection(sqlQuery,CURDtype,param);
	
	}


}
